@model List<Models.VideoViewModel>

<h1>This is the home index!</h1>

<a asp-controller="Account" asp-action="Login">Login</a>

@foreach (var item in @Model)
{
  <ul>
    <li>@item.Title | @item.UploadedBy</li>
    <li><iframe width="420" height="315"src=@item.URL></iframe> </li>
  </ul>
}